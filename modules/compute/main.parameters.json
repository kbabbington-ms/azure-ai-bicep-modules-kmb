{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "projectName": {
      "value": "ai-enclave"
    },
    "subnetId": {
      "value": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.Network/virtualNetworks/{vnet-name}/subnets/compute-subnet"
    },
    "logAnalyticsWorkspaceId": {
      "value": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.OperationalInsights/workspaces/{workspace-name}"
    },
    "vmAdminUsername": {
      "value": "azureuser"
    },
    "vmAdminPassword": {
      "value": "{secure-password}"
    },
    "vmSize": {
      "value": "Standard_D4s_v3"
    },
    "instanceCount": {
      "value": 2
    }
  }
}
